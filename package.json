{
  "name": "n8n-nodes-media-manager",
  "version": "1.0.0",
  "description": "A custom n8n node to run media automation subcommands.",
  "license": "MIT",
  "main": "dist/nodes/MediaManager/MediaManager.node.js",
  "scripts": {
    "build": "esbuild ./nodes/MediaManager/MediaManager.node.ts --bundle --platform=node --outfile=dist/nodes/MediaManager/MediaManager.node.js --external:n8n-workflow",
    "dev": "npm run build -- --watch"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/MediaManager/MediaManager.node.js"
    ]
  },
  "dependencies": {
    "@azure/storage-blob": "^12.17.0"
  },
  "devDependencies": {
    "esbuild": "^0.23.0",
    "n8n-workflow": "*",
    "typescript": "~4.8.4"
  }
}